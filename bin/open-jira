#!/usr/bin/env bash

browser=open
# Example of how to specify a specific browser. Note the space escaping...
# browser='/Applications/Google Chrome.app/Contents/MacOS/Google Chrome'

if [ -z "$1" ]; then
  echo "JIRA ticket ID required"
else
  "$browser" "https://seomoz.atlassian.net/browse/$1" 2>/dev/null
fi
